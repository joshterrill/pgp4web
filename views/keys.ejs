<!DOCTYPE html>  
<html lang="en">  
    <head>
        <% include partials/head.ejs %>
        <script src="js/bundle.js"></script>
        <script>
            $(function() {
                $("#generateKey").on("submit", function(e) {
                    e.preventDefault();
                    generate();
                });
            });
        </script>
    </head>
    <body>
        <div class="container">
            <% include partials/nav.ejs %>
            <div class="jumbotron">
                <h1>Keys</h1>
                <form id="generateKey" action="/keys" method="POST">
                    <div class="row">
                        <div class="col-md-6">
                            <input type="text" name="username" value="josht" disabled> <br />
                            <input type="text" name="keyuserid" value="Josh Terrill <joshterrill.dev@gmail.com>" disabled> <br />
                            <input type="password" name="keypassword" placeholder="Password"> <br />
                            <input type="submit" class="btn btn-success">
                        </div>
                        <div class="col-md-6">
                            <h4>Generated Key:</h4>
                            <div class="publickeyoutput"></div>
                            <div class="privatekeyoutput"></div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </body>
</html>  